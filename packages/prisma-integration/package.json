{
    "name": "prisma-rawsql",
    "version": "0.1.0-alpha",
    "description": "Prisma integration for rawsql-ts - Dynamic SQL generation with type safety and hierarchical JSON serialization",
    "main": "dist/index.js",
    "module": "dist/esm/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "test": "vitest run",
        "test:watch": "vitest",
        "coverage": "vitest run --coverage",
        "build": "tsc --build",
        "build:browser": "tsc --project tsconfig.browser.json",
        "clean": "tsc --build --clean",
        "lint": "eslint . --ext .ts,.tsx --fix"
    },
    "keywords": [
        "prisma",
        "sql",
        "rawsql-ts",
        "sql-parser",
        "sql-transformer",
        "type-safety",
        "json-serialization",
        "orm"
    ],
    "author": "msugiura",
    "license": "MIT",
    "dependencies": {
        "@prisma/internals": "^6.9.0",
        "rawsql-ts": "^0.10.5-beta"
    },
    "peerDependencies": {
        "@prisma/client": ">=4.0.0",
        "prisma": ">=4.0.0"
    },
    "devDependencies": {
        "@prisma/client": "^5.22.0",
        "@types/node": "^22.13.10",
        "@typescript-eslint/eslint-plugin": "^8.26.1",
        "@typescript-eslint/parser": "^8.26.1",
        "eslint": "^9.22.0",
        "prisma": "^5.22.0",
        "typescript": "^5.8.2",
        "vitest": "^1.5.2"
    },
    "files": [
        "dist/**/*",
        "README.md",
        "LICENSE"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/mk3008/rawsql-ts.git",
        "directory": "packages/prisma-integration"
    },
    "bugs": {
        "url": "https://github.com/mk3008/rawsql-ts/issues"
    },
    "homepage": "https://mk3008.github.io/rawsql-ts/"
}